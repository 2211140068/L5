<%= form_with url: '/top/login', method: :post, local: true do |f| %>
    <%= f.label :uid, 'ユーザーID' %>
    <%= f.text_field :uid %>
    <%= f.label :pass, 'パスワード' %>
    <%= f.password_field :pass %>
    <%= f.submit 'ログイン' %>

<% end %>
        
<%= link_to "登録へ", new_user_path %>
<%= link_to "一覧へ", users_path %>